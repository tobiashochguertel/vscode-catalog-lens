{
  "publisher": "TobiasHochguertel",
  "name": "catalog-lens",
  "displayName": "Catalog Lens (PNPM|YARN|BUN)",
  "version": "0.6.1",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "description": "Show inlay version for PNPM/Yarn/Bun catalogs",
  "author": "Tobias Hochg√ºrtel <tobias.hochguertel@googlemail.com>",
  "license": "MIT",
  "homepage": "https://github.com/tobiashochguertel/vscode-catalog-lens#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tobiashochguertel/vscode-catalog-lens"
  },
  "bugs": {
    "url": "https://github.com/tobiashochguertel/vscode-catalog-lens/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "workspaceContains:.yarnrc.yml",
    "workspaceContains:bun.lock",
    "workspaceContains:bun.lockb",
    "workspaceContains:bunfig.toml",
    "workspaceContains:pnpm-workspace.yaml"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Catalog Lens",
        "command": "pnpmCatalogLens.toggle",
        "title": "Toggle"
      },
      {
        "category": "Catalog Lens",
        "command": "pnpmCatalogLens.gotoDefinition",
        "title": "Go to Definition"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "pnpmCatalogLens.enabled": {
          "description": "Enable inlay hints",
          "type": "boolean",
          "default": true
        },
        "pnpmCatalogLens.hover": {
          "description": "Show dependency info on hover",
          "type": "boolean",
          "default": true
        },
        "pnpmCatalogLens.logLevel": {
          "description": "Log level for the extension output channel",
          "type": "string",
          "default": "INFO",
          "enum": [
            "DEBUG",
            "ERROR",
            "INFO",
            "WARNING"
          ]
        },
        "pnpmCatalogLens.namedCatalogsColors": {
          "description": "Give each named catalog a unique color",
          "type": "boolean",
          "default": true
        },
        "pnpmCatalogLens.namedCatalogsColorsSalt": {
          "description": "A random string to adding as the salt for the named catalogs colors",
          "type": "string",
          "default": "pnpm-catalog-lens"
        },
        "pnpmCatalogLens.namedCatalogsLabel": {
          "description": "Show a small label for named catalog in the inlay hint",
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "act:build": "./scripts/act-test.sh -j build -W .github/workflows/ci.yml",
    "act:ci": "./scripts/act-test.sh push -W .github/workflows/ci.yml",
    "act:lint": "./scripts/act-test.sh -j lint -W .github/workflows/ci.yml",
    "act:list": "./scripts/act-test.sh",
    "act:test": "./scripts/act-test.sh",
    "act:test-unix": "./scripts/act-test.sh -j test-unix -W .github/workflows/ci.yml",
    "act:typecheck": "./scripts/act-test.sh -j typecheck -W .github/workflows/ci.yml",
    "build": "tsdown src/index.ts --external vscode --format cjs",
    "check": "pnpm format:check && pnpm markdown:lint && pnpm lint && pnpm typecheck && pnpm build && pnpm test:unit",
    "dev": "nr build --watch",
    "ext:publish": "vsce publish --no-dependencies",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "markdown:fix": "markdownlint-cli2 \"**/*.md\" --fix",
    "markdown:lint": "markdownlint-cli2 \"**/*.md\"",
    "package": "vsce package --no-dependencies",
    "precommit": "pnpm format && pnpm markdown:fix && pnpm lint:fix && pnpm lint && pnpm typecheck && pnpm build",
    "prepare": "husky && nr update",
    "prepush": "pnpm test:unit",
    "release": "bumpp",
    "test": "vitest",
    "test:all": "vitest run --config vitest.config.ts && vitest run --config vitest.e2e.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:unit": "vitest run --config vitest.config.ts",
    "typecheck": "tsc --noEmit",
    "update": "vscode-ext-gen --output src/generated/meta.ts --scope pnpmCatalogLens",
    "vscode:prepublish": "nr build",
    "workflow:lint": "./scripts/lint-workflows.sh",
    "workflow:validate": "pnpm workflow:lint"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@antfu/ni": "^26.0.1",
    "@babel/core": "^7.28.4",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@reactive-vscode/mock": "^0.4.0",
    "@reactive-vscode/vueuse": "^0.3.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.6",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.6.0",
    "@types/vscode": "1.90.0",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.2.3",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "^1.0.2",
    "find-up": "^8.0.0",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.2.4",
    "mocha": "^10.8.2",
    "ovsx": "^0.10.5",
    "pnpm": "^10.17.1",
    "reactive-vscode": "^0.3.2",
    "rolldown": "^1.0.0-beta.41",
    "tsdown": "^0.15.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.3.0",
    "vsxpub": "^0.1.1",
    "yaml-eslint-parser": "^1.3.0"

  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "eslint --fix"
    ],
    "*.{ts,js}": [
      "pnpm typecheck"
    ]
  }
}
